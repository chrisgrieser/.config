            "header": {
                "backgroundColor": "#1D252C",
                "textColor": "#33ced8",
                "buttonColor": "#33ced8"
            },
            "notes": {
                "clean": {
                    "textColor": "#B7C5D3",
                    "backgroundColor": "#1D252C",
                    "buttonColor": "#718ca1",
                    "linkColor": "#68a1f0",
                    "secondaryTextColor": "#718ca1",
