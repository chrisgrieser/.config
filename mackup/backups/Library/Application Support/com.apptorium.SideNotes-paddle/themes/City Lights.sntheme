{
    "$schema": "https:\/\/www.apptorium.com\/schemas\/sntheme.json",
    "any": {
        "systemAppearance": "dark",
        "folders": {
            "header": {
                "backgroundColor": "#1d252c",
                "textColor": "#33ced8",
                "buttonColor": "#33ced8"
            },
            "list": {
                "backgroundColor": "#1D252C",
                "buttonColor": "#33ced8",
                "textColor": "#b7c5d3",
                "secondaryTextColor": "#718ca1"
            },
            "cleanFolderColor": "#30c4ce"
