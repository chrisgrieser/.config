                    {
                        "backgroundColor": "#252525",
                        "barColor": "#718ca1"
                    }
                ]
            }
        },
        "search": {
            "backgroundColor": "#1d252c",
            "searchField": {
                "backgroundColor": "#11161b",
                "textColor": "#b7c5d3",
                "buttonColor": "#33ced8",
                "shadow": {
                    "color": "#00000000",
                    "blurRadius": 0,
                    "offsetX": 0,
                    "offsetY": 0
