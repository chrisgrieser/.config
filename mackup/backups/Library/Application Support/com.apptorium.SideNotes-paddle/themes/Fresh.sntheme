{
    "$schema": "https:\/\/www.apptorium.com\/schemas\/sntheme.json",
    "any": {
        "systemAppearance": "light",
        "folders": {
            "header": {
                "backgroundColor": "#FFFDF1",
                "buttonColor": "#287CD6",
                "textColor": "#287CD6"
            },
            "list": {
                "backgroundColor": "#FFFDF1",
                "buttonColor": "#287CD6",
                "textColor": "#222222"
            },
            "cleanFolderColor": "#287CD6"
        },
        "folder": {
            "header": {
                "backgroundColor": "#FFFDF1",
                "buttonColor": "#287CD6",
                "textColor": "#287CD6"
            },
            "notes": {
                "clean": {
                    "textColor": "#000000dd",
                    "secondaryTextColor": "#00000070",
                    "backgroundColor": "#FFFDF1",
                    "buttonColor": "#287CD6",
                    "header": {
                        "textColor": "#287CD6"
                    },
                    "code": {
                        "textColor": "#6a1e2c"
                    },
                    "codeBlock": {
                        "textColor": "#6a1e2c"
                    },
                    "quote": {
                        "textColor": "#00000070"
                    },
                    "mark": {
                        "backgroundColor": "#59d62860"
                    },
                    "filePreview": {
                        "textColor": "#000000aa"
                    }
                },
                "colors": [
                    {
                        "backgroundColor": "#FFADA7"
                    },
                    {
                        "backgroundColor": "#B4FFA7"
                    },
                    {
                        "backgroundColor": "#A7D0FF"
                    },
                    {
                        "backgroundColor": "#FFF0A7"
                    },
                    {
                        "backgroundColor": "#B6A7FF"
                    },
                    {
                        "backgroundColor": "#CFCFCF"
                    }
                ]
            }
        },
        "search": {
            "backgroundColor": "#FFFDF1",
            "searchField": {
                "backgroundColor": "#00000011",
                "textColor": "#2b134c",
                "shadow": {
                    "backgroundColor": "#00000033",
                    "blurRadius": 0,
                    "offsetX": 0,
                    "offsetY": 0
                }
            },
            "enableVisualEffects": true
        }
    }
}
