{
  "jscommand": {
    "body": [
      "exmap ${1:ex-command-name} jscommand { ${2:one-line-of-js} }",
      "nnoremap ${3:keymap} :${1:ex-command-name}"
    ],
    "prefix": "jscommand"
  },
  "jsfile": {
    "body": [
      "exmap ${1:func-name} jsfile Meta/vimrc-jscommands.js { ${1:func-name}() }",
      "nnoremap ${2:keymap} :${1:func-name}"
    ],
    "prefix": "jsfile"
  },
  "obcommand": {
    "body": [
      "exmap ${1:ex-command-name} obcommand ${2:command-id}",
      "nnoremap ${3:keymap} :${1:ex-command-name}"
    ],
    "prefix": "obcommand"
  }
}
