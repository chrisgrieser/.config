{
  "jscommand": {
    "body": [
      "exmap ${1:ex-command-name} jscommand { ${2:one-line-of-js} }",
      "nnoremap ${3:keymap} :${1:ex-command-name}<CR>"
    ],
    "prefix": "jscommand (exmap)"
  },
  "jsfile": {
    "body": [
      "exmap ${1:func-name} jsfile Meta/vimrc-jsfile.js { ${1:func-name}() }",
      "nnoremap ${2:keymap} :${1:func-name}<CR>"
    ],
    "prefix": "jsfile (exmap)"
  },
  "obcommand": {
    "body": [
      "exmap ${3:cmd-name} obcommand ${1:command-id}",
      "nnoremap ${2:keymap} :${3:cmd-name}<CR>"
    ],
    "prefix": "obcommand"
  }
}
