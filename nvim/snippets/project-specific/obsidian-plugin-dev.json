{
	"get vim mode": {
		"body": "app.workspace.activeEditor.editor.cm.cm.state.vim.insertMode",
		"prefix": "get vim mode"
	},
	"emulate key": {
		"body": [
			"const cm = app.workspace.activeEditor.editor.cm.cm;",
			"activeWindow.CodeMirrorAdapter.Vim.handleKey(cm, \"i\", \"mapping\")"
		],
		"prefix": "emulate key"
	},
	"enter insert mode": {
		"body": [
			"const cm = app.workspace.activeEditor.editor.cm.cm;",
			"activeWindow.CodeMirrorAdapter.Vim.enterInsertMode(cm)"
		],
		"prefix": "enter insert mode"
	}
}
