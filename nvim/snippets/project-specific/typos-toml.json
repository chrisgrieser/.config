{
  "ignore file (typos)": {
    "body": [
      "[files]",
      "extend-exclude = [\"*${1:filename}\"] # * due to conform.nvim temp file"
    ],
    "prefix": "ignore file (typos)"
  },
  "ignore word (typos)": {
    "body": [
      "[default.extend-words]",
      "${1:word} = \"${1:word}\""
    ],
    "prefix": "ignore word (typos)"
  }
}
