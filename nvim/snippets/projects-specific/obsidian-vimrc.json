{
  "jscommand": {
    "body": [
      "exmap ${1:ex-command-name} jscommand { ${2:one-line-of-js} }",
      "nnoremap ${3:keymap} :${1:ex-command-name}"
    ],
    "prefix": "jscommand"
  },
  "jsfile": {
    "body": [
      "exmap ${1:func-name} jsfile Meta/vimrc-jsfile.js { ${1:func-name}() }",
      "nnoremap ${2:keymap} :${1:func-name}"
    ],
    "prefix": "jsfile"
  },
  "obcommand": {
    "body": "nnoremap ${2:keymap} :obcommand<Space>${1:command-id}<CR>",
    "prefix": "obcommand"
  }
}
