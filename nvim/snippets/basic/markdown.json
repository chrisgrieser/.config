    "prefix": "important (callout)"
  },
  "insert table 2x2": {
    "body": [
      "| ${1:Column1}   | ${2:Column2}   |",
      "|--------------- | -------------- |",
      "| ${3:Item1.1}   | ${4:Item2.1}   |"
    ],
    "prefix": "table (2x2)"
  },
  "insert toc": {
    "body": [
      "## Table of Content",
      "",
