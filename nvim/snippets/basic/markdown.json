{
	"info (GitHub Callout)": {
		"prefix": ["info (GitHub Callout)", "note (GitHub Callout)"],
		"body": ["> __Note__  ", "> $0"]
	},
	"warning (GitHub Callout)": {
		"prefix": "warning (GitHub Callout)",
		"body": ["> __Warning__  ", "> $0"]
	},
	"<kbd>": {
		"prefix": ["kbd", "<kbd>", "key"],
		"body": "<kbd>$0</kbd>",
		"description": "HTML tag for keys"
	},
	"image": {
		"prefix": "image",
		"body": "<img src=\"${1:image_path}\" alt=\"${2:alt text}\" width=\"${3:50%}\">",
		"description": "HTML syntax for images used to display images with reduced size"
	},
	"details & summary": {
		"prefix": ["toggle / fold", "details & summary"],
		"body": ["<details>", "<summary>${1:Text}</summary>", "$0", "</details>"],
		"description": "https://developer.mozilla.org/en-US/docs/Web/HTML/Element/details"
	},
	"Insert task": {
		"prefix": "task",
		"body": "- [ ] $0",
		"description": "Insert task"
	},
	"ignore (vale)": {
		"prefix": "ignore (vale)",
		"body": "<!-- vale ${1|Vale.Spelling,Google.FirstPerson,Google.Will|} = NO -->"
	},
	"ignore start/end (ltex/vale)": {
		"prefix": "ignore start/end (ltex/vale)",
		"body": ["<!-- LTeX: enabled=false --><!-- vale off -->", "<!-- LTeX: enabled=true --><!-- vale on -->"]
	},
	"ignore start/end (ltex)": {
		"prefix": "ignore start/end (ltex)",
		"body": ["<!-- LTeX: enabled=false -->", "<!-- LTeX: enabled=true -->"]
	},
	"Markdown Codeblock": {
		"prefix": [",,", "Codeblock"],
		"body": ["```${1:lang}", "$0", "```"]
	},
	"insert table (2x2)": {
		"prefix": "table (2x2)",
		"body": [
			"| ${1:Column1}   | ${2:Column2}   |",
			"|--------------- | -------------- |",
			"| ${3:Item1.1}   | ${4:Item2.1}   |",
			"${0}"
		],
		"description": "Insert table with 2 rows and 2 columns. First row is heading."
	}
}
