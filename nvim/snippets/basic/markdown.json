{
	"info (GitHub Callout)": {
		"prefix": [
			"info (GitHub Callout)",
			"note (GitHub Callout)"
		],
		"body": [
			"> __Note__  ",
			"> $0"
		]
	},
	"warning (GitHub Callout)": {
		"prefix": "warning (GitHub Callout)",
		"body": [
			"> __Warning__  ",
			"> $0"
		]
	},
	"Insert task": {
		"prefix": "task",
		"body": "- [ ] $0",
		"description": "Insert task"
	},
	"Insert 2x2 table": {
		"prefix": "2x2table",
		"body": [
			"| ${1:Column1}   | ${2:Column2}    |",
			"|--------------- | --------------- |",
			"| ${3:Item1.1}   | ${4:Item2.1}   |",
			"${0}"
		],
		"description": "Insert table with 2 rows and 2 columns. First row is heading."
	},
	"modeline: nospell": {
		"prefix": "modeline: nospell",
		"body": "<!-- vim: nospell \n-->"
	},
	"ignore (vale)": {
		"prefix": "ignore (vale)",
		"body": "<!-- vale ${1:provider.rule} = NO -->"
	}
}
