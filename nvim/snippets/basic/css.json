{
	"outline (debug)": {
		"prefix": "outline (debug)",
		"body": "outline: 2px solid red !important;"
	},
	"ignore (stylelint)": {
		"prefix": "ignore (stylelint)",
		"body": "/* stylelint-disable-line ${1:declaration-no-important} */"
	},
	"ignore range (stylelint)": {
		"prefix": "ignore range (stylelint)",
		"body": [
			"/* stylelint-disable ${1:no-duplicate-selectors} */",
			"/* stylelint-enable ${1:no-duplicate-selectors} */"
		]
	}
}
