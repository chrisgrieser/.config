{
  "DEPRECATION (+date)": {
    "body": "$LINE_COMMENT DEPRECATION (${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE})",
    "prefix": "DEPRECATION"
  },
  "DOCS (+url)": {
    "body": "$LINE_COMMENT DOCS $0$CLIPBOARD",
    "prefix": "DOCS (+url)"
  },
  "PENDING (+url)": {
    "body": "$LINE_COMMENT PENDING $0$CLIPBOARD",
    "prefix": "PENDING (+url)"
  },
  "insert ISO datestamp (today)": {
    "body": "${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
    "prefix": "insert ISO datestamp (today)"
  },
  "middle dot": {
    "body": "·",
    "prefix": "middle dot"
  },
  "no (❌)": {
    "body": "❌ ",
    "prefix": "no (❌)"
  },
  "square (■)": {
    "body": "■",
    "prefix": "square (■)"
  },
  "vim modeline (filetype)": {
    "body": "$LINE_COMMENT vim: ft=${1:filetype}",
    "prefix": "vim modeline (filetype)"
  },
  "vim modeline (foldlevel)": {
    "body": "$LINE_COMMENT vim: foldlevel=${1:1}",
    "prefix": "vim modeline (foldlevel)"
  },
  "yes (✅)": {
    "body": "✅ ",
    "prefix": "yes (✅)"
  }
}
