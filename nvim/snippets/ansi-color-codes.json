    "body": "\\e[1;37m",
    "prefix": "white"
  },
  "ansi": {
    "body": "\\e[1;38;5;${1:code}m",
    "prefix": "ansi"
  },
  "black": {
    "body": "\\e[1;30m",
    "prefix": "black"
  },
  "blue": {
    "body": "\\e[1;34m",
    "prefix": "blue"
  },
  "cyan": {
    "body": "\\e[1;36m",
    "prefix": "cyan"
  },
  "green": {
    "body": "\\e[1;32m",
    "prefix": "green"
  },
  "red": {
    "body": "\\e[1;31m",
    "prefix": "red"
  },
  "magenta": {
    "body": "\\e[1;35m",
