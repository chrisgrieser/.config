{
	"DOCS": [
		"https://code.visualstudio.com/docs/editor/userdefinedsnippets",
		"https://github.com/L3MON4D3/LuaSnip/blob/master/DOC.md#vscode"
	],
	"name": "pseudometa-snippets",
	"engines": {
		"vscode": "^1.11.0"
	},
	"contributes": {
		"snippets": [
			{
				"language": [
					"global",
					"all",
					"plaintext"
				],
				"path": "./global.json"
			},
			{
				"language": [
					"markdown"
				],
				"path": "./basic/markdown.json"
			},
			{
				"language": [
					"python"
				],
				"path": "./basic/python.json"
			},
			{
				"language": [
					"gitcommit"
				],
				"path": "./basic/gitcommit.json"
			},
			{
				"language": [
					"bash",
					"zsh",
					"fish",
					"sh"
				],
				"path": "./basic/shell.json"
			},
			{
				"language": [
					"lua"
				],
				"path": "./basic/lua.json"
			},
			{
				"language": [
					"javascript",
					"typescript"
				],
				"path": "./basic/javascript.json"
			},
			{
				"language": [
					"json"
				],
				"path": "./project-specific/vscode-snippets.json"
			},
			{
				"language": [
					"typescript"
				],
				"path": "./project-specific/obsidian-plugin-dev.json"
			},
			{
				"language": [
					"css",
					"scss"
				],
				"path": "./project-specific/obsidian-theme-dev.json"
			}
		]
	}
}
