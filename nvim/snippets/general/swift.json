  "read file": {
    "body": [
      "let filepath = \"${1:/path/to/file.txt}\"",
      "let fileUrl = URL(fileURLWithPath: (filepath as NSString).expandingTildeInPath)",
      "let contents = try String(contentsOf: fileUrl, encoding: .utf8)"
