	"keyLayout": 144,
	"keyMappings": [
		"#!no-check",
		"\" combine commands: \" https://github.com/gdh1995/vimium-c/wiki/Auto-run-a-tree-of-commands#open-vomnibar-after-creating-a-new-tab",
		"\" Google Regex above ensures keys are disabled on google sites, except the ones the google image search (\"&tbm=isch\")",
		"\" Google excludes \"o\" for Grepper",
		"\" Info on Vimium-C mapping Syntax: https://github.com/gdh1995/vimium-c/issues/873",
		"\"------------------------------------------------------",
		"",
		"unmapAll",
		"",
		"\" Modes",
		"map <esc> reset",
		"map v enterVisualMode",
		"map i enterInsertMode",
		"map V enterVisualLineMode",
		"map gi focusInput",
		"",
		"\" Find",
		"unmap -",
		"map - enterFindMode selected",
		"map n performFind",
		"map N performBackwardsFind",
		"",
		"\" Link Mode",
		"map f LinkHints.activate",
		"map b LinkHints.activateOpenInNewTab",
		"map yf LinkHints.activateCopyLinkUrl",
		"map yi LinkHints.activateCopyImage",
		"map yh LinkHints.activateHover showURL",
		"",
		"\" Tabs",
		"map q closeTabsOnLeft",
		"map e closeTabsOnRight",
		"map w removeTab mayClose",
		"map m removeTab mayClose",
		"\" -- using  then/else, since runKey does not work with the `mayClose` parameter",
		"map x closeOtherTabs $then=\"w\" $else=\"w\"",
		"map s copyCurrentUrl $then=\"w\" $else=\"w\"",
		"map u restoreTab",
		"",
		"map a previousTab",
		"map d nextTab",
		"map A moveTabLeft",
		"map D moveTabRight",
		"map t Vomnibar.activateTabSelection",
		"\" duplicate and immediately goes back to prevent auto-closing through \"Unique Tabs\" extension",
		"map yt duplicateTab  $then=\"h\" $else=\"h\"",
		"",
		"\" windows",
		"map <c-w>v moveTabToNewWindow",
		"map <c-w>m joinTabs",
		"map <c-w>i moveTabToIncognito",
		"",
		"\" Scrolling",
		"map j scrollDown",
		"map k scrollUp",
		"map J scrollDown count=4",
		"map K scrollUp count=4",
		"map gg scrollToTop",
		"map G scrollToBottom",
		"map F nextFrame",
		"",
		"\" History",
		"map h goBack",
		"map l goForward",
		"map gr Vomnibar.activateHistoryInNewTab",
		"",
		"\" Pages (not history)",
		"map H goPrevious",
		"map L goNext",
		"",
		"\" Interaction",
		"map c Vomnibar.activateEditUrl",
		"map gu goUp",
		"map gU goToRoot",
		"map p togglePinTab",
		"map o openCopiedUrlInNewTab",
		"map M toggleMuteTab",
		"map r reload",
		"map R reload hard",
		"",
		"\" Copy URLs",
		"map yy copyCurrentUrl",
		"\" copy github part of an url",
		"map yg copyCurrentUrl sed=\"s=https?://github.com/(.+?/.[^/]+).*=$1=\"",
		"map yw copyWindowInfo format=\"${url}\"",
		"",
		"\" Help",
		"map ? showHelp",
		"map , createTab url=\"chrome-extension://hfjbmagddngcpeloejdejnfgbamkjaeg/pages/options.html\"",
		""
	],
	"regexFindMode": true,
	"scrollStepSize": 300,
