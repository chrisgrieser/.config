  <Sound>
    <VolumeMusic>0.3</VolumeMusic>
    <VolumeSound>0.5</VolumeSound>
    <MuteLobbyAmbientSounds>false</MuteLobbyAmbientSounds>
  </Sound>
  <Other>
    <ModDirectories>
      <string>content</string>
    </ModDirectories>
    <DesyncCheckInterval>400</DesyncCheckInterval>
    <ControllerRumble>true</ControllerRumble>
    <ScreenShake>true</ScreenShake>
    <Blood>true</Blood>
    <AutosaveReplays>false</AutosaveReplays>
    <Controller1SharesKeyboard>true</Controller1SharesKeyboard>
