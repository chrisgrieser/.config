    <PreviousHoursShownOnLB>1</PreviousHoursShownOnLB>
    <SilentPlayerIDs />
    <InputDelay>6</InputDelay>
    <IgnoreBuildNumber>false</IgnoreBuildNumber>
  </Network>
  <Debugging>
    <LoggingLevel>INFO</LoggingLevel>
    <LogCategories>
      <Categories>ArenaEvents</Categories>
      <Categories>SteamAPI</Categories>
      <Categories>NetworkInterfaceLayer</Categories>
      <Categories>CustomLobbyEvents</Categories>
      <Categories>FMOD</Categories>
