	"grabBackFocus": true,
	"keyLayout": 154,
	"keyMappings": [
		"#!no-check",
		"\" combine commands: \" https://github.com/gdh1995/vimium-c/wiki/Auto-run-a-tree-of-commands#open-vomnibar-after-creating-a-new-tab",
		"\" Google excludes \"o\" for Grepper",
		"\" Info on Vimium-C mapping Syntax: https://github.com/gdh1995/vimium-c/issues/873",
		"\"------------------------------------------------------",
		"",
		"unmapAll",
		"",
		"\" Modes",
		"map i passNextKey",
		"map I enterInsertMode",
		"map gi focusInput select=\"end\"",
		"\" this is similar to WezTerm's QuickSelect mode",
		"map v LinkHints.activateSelect",
		"",
		"\" Find",
		"unmap -",
		"map - enterFindMode",
		"map / enterFindMode",
		"map n performFind",
		"map N performBackwardsFind",
		"",
		"\" Link Mode",
		"map f LinkHints.activate",
		"map F LinkHints.activateWithQueue",
		"map b LinkHints.activateOpenInNewTab",
		"map yf LinkHints.activateCopyLinkUrl",
		"map yi LinkHints.activateCopyImage",
		"map yd LinkHints.activateDownloadImage",
		"",
		"\" Tabs",
		"map q closeTabsOnLeft",
		"map e closeTabsOnRight",
		"map m removeTab mayClose",
		"map u restoreTab",
		"",
		"\" wasd",
		"map a previousTab",
		"map d nextTab",
		"map A moveTabLeft",
		"map D moveTabRight",
		"map w removeTab mayClose",
		"\" using then/else, since runKey does not work with the `mayClose` parameter",
		"map s copyCurrentUrl $then=\"w\" $else=\"w\"",
		"map t Vomnibar.activateTabSelection",
		"",
		"\" windows",
		"map M joinTabs",
		"map P moveTabToIncognito",
		"\" due to my window tiling, this is effectively creating a split window, thus same mapping as in nvim",
		"map <c-v> moveTabToNewWindow",
		"",
		"\" Scrolling",
		"map j scrollDown",
		"map k scrollUp",
		"map J scrollDown count=3",
		"map K scrollUp count=3",
		"map gg scrollToTop",
		"map G scrollToBottom",
		"",
		"\" History",
		"map h goBack",
		"map l goForward",
		"map gr Vomnibar.activateHistoryInNewTab",
		"",
		"\" Pages (not history)",
		"map H goPrevious",
		"map L goNext",
		"",
		"\" Interaction",
		"map c Vomnibar.activateEditUrl",
		"map gu goUp",
		"map gU goToRoot",
		"map p togglePinTab",
		"map o openCopiedUrlInNewTab",
		"map r reload",
		"map gs toggleViewSource",
		"",
		"\" Copy URLs",
		"map yy copyCurrentUrl",
		"\" copy github part of an url",
		"map yg copyCurrentUrl sed=\"s=https?://github.com/(.+?/.[^/?]+).*=$1=\"",
		"\" copy as markdown link",
		"map ym copyCurrentUrl format=\"[${title}](${url})\"",
		"",
		"\" copy links of all tabs",
		"map yw copyWindowInfo format=\"${url}\"",
		"map ya copyWindowInfo format=\"${title}\\u0020${url}\"",
		"",
		"\" Help",
		"map ? showHelp",
		"map , focusOrLaunch url=\"chrome-extension://hfjbmagddngcpeloejdejnfgbamkjaeg/pages/options.html\"",
		""
	],
	"regexFindMode": true,
	"scrollStepSize": 300,
	"searchEngines": "g https://www.google.com/search?q=%s",
	"userDefinedCss": [
