      "isEnabled" : true,
      "kind" : "Redirect",
      "sourceURLPattern" : {
        "type" : "regularExpression",
        "value" : "https:\/\/(?:x|twitter).com\/(.*)"
      },
      "title" : "X (Twitter) → Nitter (xcancel.com)",
      "type" : "originalRedirect"
    },
    {
