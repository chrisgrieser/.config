{
	"name": "Vimium C",
	"@time": "16/02/2023, 15:47:58",
	"time": 1676558878050,
	"environment": {
		"extension": "1.99.97",
		"platform": "mac",
		"chromium": 109
	},
	"allBrowserUrls": true,
	"exclusionListenHash": false,
	"exclusionRules": [
		{
			"passKeys": "",
			"pattern": ":https://mail.google.com/"
		},
		{
			"passKeys": "",
			"pattern": ":https://www.inoreader.com/"
		},
		{
			"passKeys": "j k ",
			"pattern": "^https://www.google.*/"
		}
	],
	"grabBackFocus": true,
	"keyLayout": 129,
	"keyMappings": [
		"#!no-check",
		"\" INFO: mapping assumes 'always ignore keyboard layout'",
		"\" to be enabled, meaning that z and y are switched on German Keyboard etc",
		"",
		"unmapAll",
		"",
		"\"--------------------------------",
		"",
		"\" Modes",
		"map <esc> reset",
		"map v enterVisualMode",
		"map V enterVisualLineMode",
		"map a passNextKey",
		"",
		"\" Find",
		"\" / is - due to layout ignoring",
		"map / enterFindMode",
		"map n performFind",
		"map N performBackwardsFind",
		"",
		"\" Link Mode",
		"map f LinkHints.activate",
		"map F LinkHints.activateOpenInNewTab",
		"map zf LinkHints.activateCopyLinkUrl",
		"",
		"\" Images",
		"map zi LinkHints.activateCopyImage",
		"",
		"\" Bookmarks",
		"map B addBookmark",
		"",
		"\" Tabs",
		"map wq closeTabsOnLeft",
		"map we closeTabsOnRight",
		"map wv moveTabToNewWindow",
		"map wm joinTabs",
		"map wi moveTabToIncognito",
		"map ww closeOtherTabs",
		"",
		"map i removeTab",
		"map x removeTab",
		"map u restoreTab",
		"",
		"map b previousTab",
		"map e nextTab",
		"map t Vomnibar.activateTabSelection",
		"map wn createTab window",
		"map zt duplicateTab",
		"map s sortTabs",
		"",
		"\" Scrolling",
		"map j scrollDown",
		"map k scrollUp",
		"map J scrollDown count=3",
		"map K scrollUp count=3",
		"map gg scrollToTop",
		"map G scrollToBottom",
		"",
		"\" History",
		"map h goBack",
		"map l goForward",
		"",
		"\" Pages (not history)",
		"map H goPrevious",
		"map L goNext",
		"",
		"\" URL",
		"map ge Vomnibar.activateEditUrl",
		"map gu goUp",
		"map gU goToRoot",
		"map gi focusInput",
		"map gs toggleViewSource",
		"map zz copyCurrentUrl",
		"map p openCopiedUrlInCurrentTab",
		"map P openCopiedUrlInNewTab",
		"map m toggleMuteTab",
		"",
		"\" Reload",
		"map r reload",
		"map R reload hard",
		"",
		"\"Misc _ = ? in us layout",
		"map _ showHelp",
		""
	],
	"regexFindMode": true,
	"scrollStepSize": 300,
	"searchEngines": "g https://www.google.com/search?q=%s",
	"vimSync": true,
	"ignoreKeyboardLayout": 2,
	"mapModifier": 2
}
