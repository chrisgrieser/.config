{
	"name": "Vimium C",
	"@time": "26/02/2023, 00:19:25",
	"time": 1677367165455,
	"environment": {
		"extension": "1.99.97",
		"platform": "mac",
		"chromium": 110
	},
	"allBrowserUrls": true,
	"exclusionListenHash": false,
	"exclusionRules": [
		{
			"passKeys": "",
			"pattern": ":https://mail.google.com/"
		},
		{
			"passKeys": "",
			"pattern": ":https://www.inoreader.com/"
		},
		{
			"passKeys": "j k ",
			"pattern": "^https:\\/\\/www\\.google(?:.(?!&tbm=isch))*$"
		},
		{
			"passKeys": "? ",
			"pattern": ":https://github.com/"
		}
	],
	"grabBackFocus": true,
	"keyLayout": 144,
	"keyMappings": [
		"#!no-check",
		"\" combine commands: \" https://github.com/gdh1995/vimium-c/wiki/Auto-run-a-tree-of-commands#open-vomnibar-after-creating-a-new-tab",
		"unmapAll",
		"\" Google Regex above ensures keys are disabled on googel sites, except the ones the google image search (\"&tbm=isch\")",
		"\"------------------------------------------------------",
		"",
		"",
		"\" Modes",
		"map <esc> reset",
		"map v enterVisualMode",
		"map V enterVisualLineMode",
		"map a passNextKey",
		"",
		"\" Find",
		"unmap -",
		"map - enterFindMode",
		"map n performFind",
		"map N performBackwardsFind",
		"",
		"\" Link Mode",
		"map f LinkHints.activate",
		"map F LinkHints.activateOpenInNewTab",
		"map yf LinkHints.activateCopyLinkUrl",
		"map yi LinkHints.activateCopyImage",
		"",
		"\" Tabs",
		"map wq closeTabsOnLeft",
		"map we closeTabsOnRight",
		"map ww closeOtherTabs",
		"map wv moveTabToNewWindow",
		"map wm joinTabs",
		"map wi moveTabToIncognito",
		"",
		"map i removeTab",
		"map u restoreTab",
		"map yt duplicateTab",
		"",
		"map b previousTab",
		"map e nextTab",
		"map B moveTabLeft",
		"map E moveTabRight",
		"map gb Vomnibar.activateTabSelection",
		"map t Vomnibar.activateTabSelection",
		"",
		"\" Scrolling",
		"map j scrollDown",
		"map k scrollUp",
		"map J scrollDown count=3",
		"map K scrollUp count=3",
		"map gg scrollToTop",
		"map G scrollToBottom",
		"",
		"\" History",
		"map h goBack",
		"map l goForward",
		"map gr Vomnibar.activateHistoryInNewTab",
		"",
		"\" Pages (not history)",
		"map H goPrevious",
		"map L goNext",
		"",
		"\" Interaction",
		"map gi focusInput",
		"map ge Vomnibar.activateEditUrl",
		"map gu goUp",
		"map gU goToRoot",
		"map p togglePinTab",
		"map m toggleMuteTab",
		"map r reload",
		"map R reload hard",
		"",
		"\" Copy URLs",
		"map yy copyCurrentUrl",
		"map Y copyWindowInfo format=\"${url}\"",
		"map q runKey keys=\"copyCurrentUrl+removeTab\"",
		"",
		"\" Help",
		"map ? showHelp",
		""
	],
	"regexFindMode": true,
	"scrollStepSize": 300,
	"searchEngines": "g https://www.google.com/search?q=%s",
	"userDefinedCss": [
		".HUD { ",
		"  max-width: 80vw;",
		"  width: auto;",
		"  min-width: 600px;",
		"  height: 21px;",
		"  line-height: 1;",
		"  font-size: 18px;",
		"  padding: 6px;",
		"}",
		""
	],
	"vimSync": true,
	"ignoreCapsLock": 2,
	"mapModifier": 2
}
