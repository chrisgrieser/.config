{
	"name": "Vimium C",
	"@time": "24/02/2023, 22:11:17",
	"time": 1677273077405,
	"environment": {
		"extension": "1.99.97",
		"platform": "mac",
		"chromium": 110
	},
	"allBrowserUrls": true,
	"exclusionListenHash": false,
	"exclusionRules": [
		{
			"passKeys": "",
			"pattern": ":https://mail.google.com/"
		},
		{
			"passKeys": "",
			"pattern": ":https://www.inoreader.com/"
		},
		{
			"passKeys": "j k ",
			"pattern": "^https://www.google.*/"
		},
		{
			"passKeys": "? ",
			"pattern": ":https://github.com/"
		}
	],
	"grabBackFocus": true,
	"keyLayout": 144,
	"keyMappings": [
		"#!no-check",
		"unmapAll",
		"",
		"\" Modes",
		"map <esc> reset",
		"map v enterVisualMode",
		"map V enterVisualLineMode",
		"map a passNextKey",
		"",
		"\" Find",
		"unmap -",
		"map - enterFindMode",
		"map n performFind",
		"map N performBackwardsFind",
		"",
		"\" Link Mode",
		"map f LinkHints.activate",
		"map F LinkHints.activateOpenInNewTab",
		"map yf LinkHints.activateCopyLinkUrl",
		"map yi LinkHints.activateCopyImage",
		"",
		"\" Tabs",
		"map wq closeTabsOnLeft",
		"map we closeTabsOnRight",
		"map ww closeOtherTabs",
		"map wv moveTabToNewWindow",
		"map wm joinTabs",
		"map wi moveTabToIncognito",
		"",
		"map i removeTab",
		"map x removeTab",
		"map u restoreTab",
		"map yt duplicateTab",
		"",
		"map b previousTab",
		"map e nextTab",
		"map B moveTabLeft",
		"map E moveTabRight",
		"map gb Vomnibar.activateTabSelection",
		"map t Vomnibar.activateTabSelection",
		"",
		"\" Scrolling",
		"map j scrollDown",
		"map k scrollUp",
		"map J scrollDown count=3",
		"map K scrollUp count=3",
		"map gg scrollToTop",
		"map G scrollToBottom",
		"",
		"\" History",
		"map h goBack",
		"map l goForward",
		"map gr Vomnibar.activateHistoryInNewTab",
		"",
		"\" Pages (not history)",
		"map H goPrevious",
		"map L goNext",
		"",
		"\" Interaction",
		"map ge Vomnibar.activateEditUrl",
		"map gu goUp",
		"map gU goToRoot",
		"map gs toggleViewSource",
		"map yy copyCurrentUrl",
		"map Y copyWindowInfo format=\"${url}\"",
		"map p togglePinTab",
		"map m toggleMuteTab",
		"map r reload",
		"map R reload hard",
		"map gi focusInput",
		"",
		"\" Help",
		"map ? showHelp",
		""
	],
	"regexFindMode": true,
	"scrollStepSize": 300,
	"searchEngines": "g https://www.google.com/search?q=%s",
	"vimSync": true,
	"ignoreCapsLock": 2,
	"mapModifier": 2
}
