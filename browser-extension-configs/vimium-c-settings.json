{
	"name": "Vimium C",
	"@time": "19/02/2023, 15:05:21",
	"time": 1676815521136,
	"environment": {
		"extension": "1.99.97",
		"platform": "mac",
		"chromium": 110
	},
	"allBrowserUrls": true,
	"exclusionListenHash": false,
	"exclusionRules": [
		{
			"passKeys": "",
			"pattern": ":https://mail.google.com/"
		},
		{
			"passKeys": "",
			"pattern": ":https://www.inoreader.com/"
		},
		{
			"passKeys": "j k ",
			"pattern": "^https://www.google.*/"
		}
	],
	"grabBackFocus": true,
	"keyLayout": 129,
	"keyMappings": [
		"#!no-check",
		"\" INFO: mapping assumes 'always ignore keyboard layout'",
		"\" to be enabled, meaning that z and y are switched on German Keyboard etc",
		"",
		"unmapAll",
		"",
		"\"--------------------------------",
		"",
		"\" Modes",
		"map <esc> reset",
		"map v enterVisualMode",
		"map V enterVisualLineMode",
		"map a passNextKey",
		"",
		"\" Find (/ is - due to US layout)",
		"map / enterFindMode",
		"map n performFind",
		"map N performBackwardsFind",
		"",
		"\" Link Mode",
		"map f LinkHints.activate",
		"map F LinkHints.activateOpenInNewTab",
		"map zf LinkHints.activateCopyLinkUrl",
		"map zi LinkHints.activateCopyImage",
		"",
		"\" Tabs",
		"map wq closeTabsOnLeft",
		"map we closeTabsOnRight",
		"map ww closeOtherTabs",
		"map wv moveTabToNewWindow",
		"map wm joinTabs",
		"map wi moveTabToIncognito",
		"",
		"map i removeTab",
		"map x removeTab",
		"map u restoreTab",
		"map zt duplicateTab",
		"",
		"map b previousTab",
		"map e nextTab",
		"map B moveTabLeft",
		"map E moveTabRight",
		"map gb Vomnibar.activateTabSelection",
		"map t Vomnibar.activateTabSelection",
		"",
		"\" Scrolling",
		"map j scrollDown",
		"map k scrollUp",
		"map J scrollDown count=3",
		"map K scrollUp count=3",
		"map gg scrollToTop",
		"map G scrollToBottom",
		"",
		"\" History",
		"map h goBack",
		"map l goForward",
		"map gr Vomnibar.activateHistoryInNewTab",
		"",
		"\" Pages (not history)",
		"map H goPrevious",
		"map L goNext",
		"",
		"\" URL",
		"map ge Vomnibar.activateEditUrl",
		"map gu goUp",
		"map gU goToRoot",
		"map gs toggleViewSource",
		"map zz copyCurrentUrl",
		"map p togglePinTab",
		"map m toggleMuteTab",
		"map r reload",
		"map R reload hard",
		"",
		"\" Misc (_ = ? in US layout)",
		"map _ showHelp",
		"map s addBookmark",
		"map gi focusInput",
		""
	],
	"regexFindMode": true,
	"scrollStepSize": 300,
	"searchEngines": "g https://www.google.com/search?q=%s",
	"vimSync": true,
	"ignoreKeyboardLayout": 2,
	"mapModifier": 2
}
