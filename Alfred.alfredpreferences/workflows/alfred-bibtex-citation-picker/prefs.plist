<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>bibtex_library_path</key>
	<string>~/.config/pandoc/main-bibliography.bib</string>
	<key>citation_picker_keyword</key>
	<string>cc</string>
	<key>copy_citation_on_adding_entry</key>
	<true/>
	<key>csl_folder</key>
	<string>~/.config/pandoc/csl</string>
	<key>literature_note_folder</key>
	<string>~/vaults/main-vault/Literature Notes</string>
	<key>match_year_type</key>
	<string>short+full</string>
	<key>open_entries_in</key>
	<string>default app for .bib files</string>
	<key>pdf_folder</key>
	<string>~/Library/Mobile Documents/com~apple~CloudDocs/Academia/PDFs</string>
</dict>
</plist>
