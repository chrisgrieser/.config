				<string>ObjC.import('stdlib');

function run(argv) {
  let folderID = argv[0];
  let text = $.getenv('text');
  let isPath = $.getenv('isPath');
  let app = Application("SideNotes");

  app.createNote({
	"folder": app.folders.whose({"id": folderID})[0](),
	"text": text,
	"ispath": isPath
  });    

  return text;
}</string>
