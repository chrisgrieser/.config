				<string>function run(argv) {
    var query = argv[0] == undefined ? "" : argv[0].trim();
    let app = Application("SideNotes");
    let queryResults = app.searchFolders(query);

    var results = [];

    for (let res of queryResults) {
    	results.push({
    	    "uid": res.id(),
    		"title": res.name(),
    		"subtitle": res.name() + " folder",
			"arg": res.id(),
			"action": {
				"text": res.name()
			}
    	});
    }

    return JSON.stringify({
    	  "items": results
    });
}
