				<key>script</key>
				<string>function run(argv) {
    var menuItemName = argv[0];
    
    // Show the context menu
    Application("System Events").processes["Alfred Preferences"].windows["Alfred Preferences"]
        .splitterGroups[0].scrollAreas[0].tables[0].actions["AXShowMenu"].perform();
    
    delay(0.3);
    
    // Click menu item with specified name
    Application("System Events").processes["Alfred Preferences"].windows["Alfred Preferences"]
        .splitterGroups[0].scrollAreas[0].tables[0].menus[0].menuItems[menuItemName]
        .actions["AXPress"].perform();
    
    return;
}</string>
